                               (SCALE-ARRAY OMEGA P-ALPHA)
                               P-OMEGA))))))
      (IF (AND)
          (FUNCALL
           #'(LAMBDA (OMEGA ALPHA)
               (RESHAPE-ARRAY-FITTING OMEGA (ARRAY-TO-LIST ALPHA)))
           (IF (AND)
               (FUNCALL
                #'(LAMBDA (OMEGA)
                    (MAKE-ARRAY (LIST OMEGA) :INITIAL-CONTENTS
                                (IOTA OMEGA :START
                                      (OF-STATE *APRIL-IDIOM* :COUNT-FROM))))
                (IF (ARRAYP (VECTOR 9))
                    (DISCLOSE (VECTOR 9))
                    (VECTOR 9))))
           (VECTOR 2 3)))
      (IF (AND)
          (FUNCALL
           #'(LAMBDA (OMEGA ALPHA)
               (RESHAPE-ARRAY-FITTING OMEGA (ARRAY-TO-LIST ALPHA)))
           (IF (AND)
