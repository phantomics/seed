                  (IF (AND AXES
                           (> 0
                              (- (AREF (FIRST AXES) 0)
                                 (OF-STATE *APRIL-IDIOM* :COUNT-FROM))))
                      (ERROR
                       (FORMAT NIL "Specified axis is less than ~a."
                               (OF-STATE *APRIL-IDIOM* :COUNT-FROM)))
                      (CONCATENATE 'VECTOR ALPHA OMEGA)))
              (IF (OR (NOT AXES) (INTEGERP (AREF (FIRST AXES) 0)))
                  (LET* ((AXIS
                          (IF AXES
                              (- (AREF (FIRST AXES) 0)
                                 (OF-STATE *APRIL-IDIOM* :COUNT-FROM))
                              (1-
                               (MAX (ARRAY-RANK ALPHA) (ARRAY-RANK OMEGA)))))
