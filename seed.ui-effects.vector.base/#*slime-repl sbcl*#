    2 (:AXES (3 2)) (:FN #\,))))
 (IF (AND)
     (FUNCALL
      #'(LAMBDA (OMEGA &OPTIONAL AXES)
          (RAVEL (OF-STATE *APRIL-IDIOM* :COUNT-FROM) OMEGA AXES))
      (IF (AND)
          (FUNCALL
           #'(LAMBDA (OMEGA ALPHA)
               (RESHAPE-ARRAY-FITTING OMEGA (ARRAY-TO-LIST ALPHA)))
           (IF (AND)
               (FUNCALL
                #'(LAMBDA (OMEGA)
                    (MAKE-ARRAY (LIST OMEGA) :INITIAL-CONTENTS
                                (IOTA OMEGA :START
                                      (OF-STATE *APRIL-IDIOM* :COUNT-FROM))))
                (IF (ARRAYP (VECTOR 12))
                    (DISCLOSE (VECTOR 12))
                    (VECTOR 12))))
