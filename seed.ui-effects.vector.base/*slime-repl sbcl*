             (DISCLOSE
              (IF (AND)
                  (FUNCALL
                   #'(LAMBDA (OMEGA ALPHA &OPTIONAL AXES)
                       (IF (AND
                            (OR (NOT AXES) (INTEGERP (AREF (FIRST AXES) 0)))
                            (VECTORP ALPHA) (VECTORP OMEGA))
